{
  "scripts": {
    "clean": "rimraf ./public/js ./public/css ./public/fonts ./public/workbox ./public/workbox-sw.js ./public/index.html ./public/firebase-messaging-sw.js ./public/firebase-messaging-sw.js.map",
    "build": "npm run clean && npm run createfirebaseconf && npm install && webpack -p && echo 'Done. Deploy Success.'",
